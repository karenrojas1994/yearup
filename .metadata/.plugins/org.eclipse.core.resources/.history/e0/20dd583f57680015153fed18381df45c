
public class Circle 
{
String m_color = null;
double m_radius = -1;

	public static void main(String[] args)
	{
		Circle bigCircle = new Circle(90);
		System.out.println("Big cirlce's radius is" + bigCircle.m_radius);
		System.out.println("Big ciecle's color is " + bigCircle.m_color);
		System.out.println("Big cirlce's area is " + bigCircle.CalculateArea());
		
	
}
Circle(int radius)
{
	m_radius = radius;
	m_color = "grey";
}

Circle(int radius, String color)
{
m_radius = radius;
m_color = color;

}
double CalculateArea()
{
	double area = 3.14 * m_radius * m_radius;
	return area;
	
}
}
